<%= simple_form_for(@work_schedule) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :company %>
    <%= f.input :year %>
    <%= f.association :holiday %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
